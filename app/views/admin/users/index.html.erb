<% @users.each do |user| %>
<h3>Full name: <%= user.full_name %></h3>
<p>Display name: <%= user.display_name %></p>
<p>Email: <%= user.email %></p>
<p><%= link_to "Edit", edit_admin_user_path(user) %></p>
<p><%= link_to "Delete this account", admin_user_path(user), method: :delete, data: {confirm:"Really DELETE this account?"}%></p>
<% end %>
